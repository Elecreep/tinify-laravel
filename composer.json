{
  "name": "jargoud/tinify-laravel",
  "type": "library",
  "description": "Tinify API support with Laravel",
  "keywords": [
    "tinify",
    "tinypng",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "yasmuru",
      "email": "yasmuru@gmail.com"
    },
    {
      "name": "Jérémy Argoud",
      "email": "jeremy.argoud@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "illuminate/support": "^5.2|^6.0|^7.0|^8.0",
    "tinify/tinify": "^2.0"
  },
  "require-dev": {
    "phpro/grumphp": "^1.1",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "yasmuru\\LaravelTinify\\": "src"
    }
  },
  "minimum-stability": "stable",
  "repositories": {
    "tinify-php": {
      "type": "vcs",
      "url": "https://github.com/jargoud/tinify-php"
    }
  }
}
