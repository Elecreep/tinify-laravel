{
    "name": "jargoud/tinify-laravel",
    "type": "library",
    "description": "Tinify API support with Laravel",
    "keywords": [
        "tinify",
        "tinypng",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "yasmuru",
            "email": "yasmuru@gmail.com"
        },
        {
            "name": "Jérémy Argoud",
            "email": "jeremy.argoud@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/bus": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/queue": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "jargoud/tinify-php": "^2.0"
    },
    "require-dev": {
        "phpro/grumphp": "^1.1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jargoud\\LaravelTinify\\": "src"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:jargoud/tinify-php.git"
        }
    ]
}
